            function modifyFontSize(flage) {
                var divElement = $('#divContent');
                var CurrentFontSize = parseInt(divElement.css('font-size'));
                if (flage == 'Increase') {
                    if (CurrentFontSize < 20) {
                        CurrentFontSize += 1;
                        $('#decreaseSpan').text('');
                        $('#increaseSpan').text(CurrentFontSize);
                    }
                }
                else if (flage == 'Decrease') {
                    if (CurrentFontSize > 10) {
                        CurrentFontSize -= 1;
                        $('#increaseSpan').text('');
                        $('#decreaseSpan').text(CurrentFontSize);
                    }
                }
                else {
                    CurrentFontSize = 16;
                }
                $('#divContent').css('font-size', CurrentFontSize);

            }

        });
       
    </script>






    
</body>
