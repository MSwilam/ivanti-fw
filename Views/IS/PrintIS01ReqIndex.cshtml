                <p> </p>
                <p> </p>
            </div>
        </div>
        <div class="wrapper-table">
            <table class="MainTable" style="margin:0px;padding:0px;">

                <thead>
                    <tr class="theadResp">



                        <th class="DetailsDialog th1 " >@Html.GSText("USERID")</th>
	<th class="DetailsDialog th2 " >@Html.GSText("USERNAME")</th>
	<th class="DetailsDialog th3 " >@Html.GSText("ENTITYNAME")</th>
	<th class="DetailsDialog th4 " style="display:none;">@Html.GSText("EMAIL")</th>
	<th class="DetailsDialog th5 " style="display:none;">@Html.GSText("INCIDENTINFORMATION")</th>
	<th class="DetailsDialog th6 " style="display:none;">@Html.GSText("IncidentType")</th>
	<th class="DetailsDialog th7 " style="display:none;">@Html.GSText("INCIDENTDATE")</th>
	<th class="DetailsDialog th8 " style="display:none;">@Html.GSText("INCIDENTTIME")</th>
	<th class="DetailsDialog th9 " style="display:none;">@Html.GSText("IncidentHandled")</th>
	<th class="DetailsDialog th10 " style="display:none;">@Html.GSText("HANDLINGTIME")</th>
	<th class="DetailsDialog th11 " style="display:none;">@Html.GSText("CybersecurityIncidentCalculator")</th>
	<th class="DetailsDialog th12 " style="display:none;">@Html.GSText("ServiceProviderClassification")</th>
	<th class="DetailsDialog th13 " style="display:none;">@Html.GSText("ImpactedService")</th>
	<th class="DetailsDialog th14 " style="display:none;">@Html.GSText("SophisticationAttack")</th>
	<th class="DetailsDialog th15 " style="display:none;">@Html.GSText("ImpactTypeCompromise")</th>
	<th class="DetailsDialog th16 " style="display:none;">@Html.GSText("ImpactTypeDataLeakage")</th>
	<th class="DetailsDialog th17 " style="display:none;">@Html.GSText("ImpactTypeReputation")</th>
	<th class="DetailsDialog th18 " style="display:none;">@Html.GSText("ImpactTypeDisruption")</th>
	<th class="DetailsDialog th19 " style="display:none;">@Html.GSText("ImpactTypeDestruction")</th>
	<th class="DetailsDialog th20 " style="display:none;">@Html.GSText("ImpactAsset")</th>
	<th class="DetailsDialog th21 " style="display:none;">@Html.GSText("PublishedExternally")</th>
	<th class="DetailsDialog th22 " style="display:none;">@Html.GSText("RootCause")</th>
	<th class="DetailsDialog th23 " style="display:none;">@Html.GSText("AttackStatus")</th>
	<th class="DetailsDialog th24 " style="display:none;">@Html.GSText("TOTALSCORE")</th>
	<th class="DetailsDialog th25 " style="display:none;">@Html.GSText("ImpactIncident")</th>
	<th class="DetailsDialog th26 " style="display:none;">@Html.GSText("ImpactType")</th>
	<th class="DetailsDialog th27 " style="display:none;">@Html.GSText("ENTITESINFORMED")</th>
	<th class="DetailsDialog th28 " style="display:none;">@Html.GSText("IsDamagedSystem")</th>
	<th class="DetailsDialog th29 " style="display:none;">@Html.GSText("DAMAGEDSYSTEM")</th>
	<th class="DetailsDialog th30 " style="display:none;">@Html.GSText("ACTIONALREADYTAKEN")</th>
	<th class="DetailsDialog th31 " style="display:none;">@Html.GSText("IOCS")</th>
	




                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model.IS01List)
                    {
                    <tr class="bodytr">
                       <td class="DetailsDialog th1 "  >@item.UserID</td>
	<td class="DetailsDialog th2 "  >@item.UserName</td>
	<td class="DetailsDialog th3 "  >@item.EntityName</td>
	<td class="DetailsDialog th4 " style="display:none;" >@item.Email</td>
	<td class="DetailsDialog th5 " style="display:none;" >@item.IncidentInformation</td>
	<td class="DetailsDialog th6" style="display:none;">@EA.GetIncidentType(@item.IncidentTypeId) </td>
	<td class="DetailsDialog th7 " style="display:none;" >@item.IncidentDate</td>
	<td class="DetailsDialog th8 " style="display:none;" >@item.IncidentTime</td>
	<td class="DetailsDialog th9" style="display:none;">@EA.GetYesNoAnswer(@item.IncidentHandled) </td>
	<td class="DetailsDialog th10 " style="display:none;" >@item.HandlingTime</td>
	<td class="DetailsDialog th11" style="display:none;">@EA.GetYesNoAnswer(@item.CybersecurityIncidentCalculator) </td>
	<td class="DetailsDialog th12" style="display:none;">@EA.GetServiceProviderClassification(@item.ServiceProviderClassificationId) </td>
	<td class="DetailsDialog th13" style="display:none;">@EA.GetImpactedService(@item.ImpactedServiceId) </td>
	<td class="DetailsDialog th14" style="display:none;">@EA.GetSophisticationAttack(@item.SophisticationAttackId) </td>
	<td class="DetailsDialog th15" style="display:none;">@EA.GetImpactTypeSeverity(@item.ImpactTypeCompromiseId) </td>
	<td class="DetailsDialog th16" style="display:none;">@EA.GetImpactTypeSeverity(@item.ImpactTypeDataLeakageId) </td>
	<td class="DetailsDialog th17" style="display:none;">@EA.GetImpactTypeSeverity(@item.ImpactTypeReputationId) </td>
	<td class="DetailsDialog th18" style="display:none;">@EA.GetImpactTypeSeverity(@item.ImpactTypeDisruptionId) </td>
	<td class="DetailsDialog th19" style="display:none;">@EA.GetImpactTypeSeverity(@item.ImpactTypeDestructionId) </td>
	<td class="DetailsDialog th20" style="display:none;">@EA.GetImpactAsset(@item.ImpactAssetId) </td>
	<td class="DetailsDialog th21" style="display:none;">@EA.GetPublishedExternally(@item.PublishedExternallyId) </td>
	<td class="DetailsDialog th22" style="display:none;">@EA.GetRootCause(@item.RootCauseId) </td>
	<td class="DetailsDialog th23" style="display:none;">@EA.GetAttackStatus(@item.AttackStatusId) </td>
	<td class="DetailsDialog th24 " style="display:none;" >@item.TotalScore</td>
	<td class="DetailsDialog th25" style="display:none;">@EA.GetImpactIncident(@item.ImpactIncidentId) </td>
	<td class="DetailsDialog th26" style="display:none;">@EA.GetImpactType(@item.ImpactTypeId) </td>
	<td class="DetailsDialog th27 " style="display:none;" >@item.EntitesInformed</td>
	<td class="DetailsDialog th28" style="display:none;">@EA.GetYesNoAnswer(@item.IsDamagedSystem) </td>
	<td class="DetailsDialog th29 " style="display:none;" >@item.DamagedSystem</td>
	<td class="DetailsDialog th30 " style="display:none;" >@item.ActionAlreadyTaken</td>
	<td class="DetailsDialog th31 " style="display:none;" >@item.Iocs</td>
	
                    </tr>
                    }



                    <tr class="FooterPage">
                        <th class="FinalPage trFullWidth" colspan="TOTAL_COUNT">
                            <div class="divbox headerRight">
