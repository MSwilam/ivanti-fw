@model Ivanti_FW.Models.Is05ModelReq

@{
    ViewBag.Title = EA.GetMessage("CREATE");
    //Layout = "~/Views/Shared/_LayoutWork.cshtml";
}


<style>
    .tooltip-master {
        display: none;
    }
</style>

@section toolbar{

    <div class="page-header mb-0">
        <div class="col-md-12">
            <div class="row align-items-end">
                <div class="col-xl-9">
                    <div class="d-flex page-header-element align-items-center">
                        <img src="//cdn.saudisystems.net/V4/assets/images/svgs/service_line/Indv/sims.svg" width="64" height="64" class="me-2">
                        <div>
                            <a href="/IS/Is05dashboard" class="back_btn pb-1 align-items-end mt-0 fs-12">
                                <i class="mutasil-icon mutasil-icon-40 fa-lg"></i>
                                @Html.GSText("BACK_TO_Is05")
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}


@{
	 var  YesNoAnswerCollection = EA.GetYesNoAnswer(); 
	 var  SeverityLevelCollection = EA.GetSeverityLevel(); 
	 var  IRTypeCollection = EA.GetIRType(); 
	 var  ClosureReasonCollection = EA.GetClosureReason(); 
	}
	

<form asp-action="CreateIs05" asp-controller="IS" method="POST" data-ajax="true" data-ajax-complete="fnCompleted" class="CreatePage" id="Is05FrmID">

    @Html.AntiForgeryToken()


                            <!-- form start -->
                            <section class="form-horizontal center">
                                <div class="box-body">
                                    <div class="form-group">
	<label class="col-sm-4 control-label">@Html.GSText("YesNoAnswer")</label>
		<div class="col-sm-8">
			@Html.ValidationMessageFor(model => model.Is05.NotifyNCA, "", new { @class = "text-danger" })
			@Html.DropDownListFor((m) => m.Is05.NotifyNCA, new SelectList(YesNoAnswerCollection, "Key", "Value"), string.Format("{0} - {1}", EA.GetMessage("YesNoAnswer"), EA.GetMessage("All")), new { @class = "form-control input-sm" })
		</div>
	</div>
	<div class="form-group">
	<label class="col-sm-4 control-label">@Html.GSText("NotifyDMO")</label>
		<div class="col-sm-8">
			@Html.ValidationMessageFor(model => model.Is05.NotifyDMO, "", new { @class = "text-danger" })
			@Html.DropDownListFor((m) => m.Is05.NotifyDMO, new SelectList(YesNoAnswerCollection, "Key", "Value"), string.Format("{0} - {1}", EA.GetMessage("YesNoAnswer"), EA.GetMessage("All")), new { @class = "form-control input-sm" })
		</div>
	</div>
