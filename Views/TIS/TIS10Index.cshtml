@model Ivanti_FW.Models.TIS10SearchModel


@{
	 var  YesNoAnswerCollection = EA.GetYesNoAnswer(); 
	 var  ThreatClassificationCollection = EA.GetThreatClassification(); 
	 var  ClosureReasonCollection = EA.GetClosureReason(); 
	}
	

<div class="search-form" id="div-search-emp">

    <form url="@Url.Action("TIS10ReqIndex")" id="TIS10SubmitIndex">
        <div class="row search-sec">
  	<div class="col-lg-12">
 		<div class="row">
		 <div class="col-lg-3 col-md-3 col-sm-12 p-0">
		 @Html.DropDownListFor((m) => m.TIS10.RelevantToSector, new SelectList(YesNoAnswerCollection, "Key", "Value"), EA.GetMessage("All"), new { @class = "form-control input-sm" })
	 </div>
		 <div class="col-lg-3 col-md-3 col-sm-12 p-0">
		 @Html.DropDownListFor((m) => m.TIS10.ThreatClassificationId, new SelectList(ThreatClassificationCollection, "Key", "Value"), EA.GetMessage("All"), new { @class = "form-control input-sm" })
	 </div>
		 <div class="col-lg-3 col-md-3 col-sm-12 p-0">
		 @Html.DropDownListFor((m) => m.TIS10.VulnerabilityIdentified, new SelectList(YesNoAnswerCollection, "Key", "Value"), string.Format("{0} - {1}", EA.GetMessage("VulnerabilityIdentified"), EA.GetMessage("All")), new { @class = "form-control input-sm" })
	 </div>
		 <div class="col-lg-3 col-md-3 col-sm-12 p-0">
		 @Html.DropDownListFor((m) => m.TIS10.ClosureReasonId, new SelectList(ClosureReasonCollection, "Key", "Value"), EA.GetMessage("All"), new { @class = "form-control input-sm" })
	 </div>
		 <div class="col-lg-3 col-md-3 col-sm-12 p-0">
		 @Html.DropDownListFor((m) => m.TIS10.FYINedded, new SelectList(YesNoAnswerCollection, "Key", "Value"), string.Format("{0} - {1}", EA.GetMessage("FYINedded"), EA.GetMessage("All")), new { @class = "form-control input-sm" })
	 </div>
		
  		<div class="col-lg-3 col-md-3 col-sm-12 p-0">

						<button type="button" class="btn btn-primary wrn-btn btn-serach-submit"> <i class="fa fa-search" aria-hidden="true"></i> @Html.GSText("Search")</button>
                                <button type="button" formaction="@Url.Action("PrintTIS10ReqIndex", "TIS")" class="btn btn-default wrn-btn  GSSubmit"><i class="fa fa-print no-print" aria-hidden="true"></i>@Html.GSText("Print")</button>
                            </div>
				</div>
                    </div>
                </div>
               @Html.HiddenFor((m) => m.FieldType)
                    @Html.HiddenFor((m) => m.PageIndex)
                    @Html.HiddenFor((m) => m.PageSize)
                    @Html.HiddenFor((m) => m.SortField)
                    @Html.HiddenFor((m) => m.SortDirection)
		    
    </form>

    <div class="search-results" id="div-search-results">

    </div>


</div>

<script type="text/javascript">
    SubmitIndex($('#TIS10SubmitIndex'));
</script>













